{
  "manifest_version": 3,
  "name": "Demarest GPA Calculator",
  "description": "This extension calculates the GPA for Northern Valley Regional High School at Demarest Students",
  "version": "1.2",
  "author": "Inpyo Lee",
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://*.genesisedu.net/*tab3=weeklysummary*"
      ],
      "js": [
        "./src/jquery.min.js",
        "./src/content_script.js"
      ],
      "css": [
        "./styles/loader.css"
      ],
      "run_at": "document_end"
    }
  ],
  "action": {
    "default_icon": "./images/icon.png",
    "default_title": "Demarest GPA",
    "default_popup": "./popup.html"
  }
}